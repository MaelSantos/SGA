               <children>
                  <Label text="Histórico: " textFill="WHITE">
                     <font>
                        <Font name="Verdana Bold" size="24.0" />
                     </font>
                  </Label>
                  <Label fx:id="lblData" text="De: - Até:" textFill="WHITE">
                     <font>
                        <Font name="Verdana Bold" size="24.0" />
                     </font>
                  </Label>
               </children>
               <VBox.margin>
                  <Insets left="10.0" />
               </VBox.margin>
            </FlowPane>
            <GridPane alignment="CENTER" hgap="5.0" prefHeight="58.0" prefWidth="674.0" vgap="10.0">
              <columnConstraints>
                  <ColumnConstraints maxWidth="438.0" minWidth="10.0" prefWidth="40.0" />
