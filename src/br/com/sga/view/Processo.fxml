<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="700.0" style="-fx-background-color: #008B8B;" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.sga.controle.ControleProcesso">
   <children>
      <VBox alignment="CENTER_LEFT" prefHeight="600.0" prefWidth="700.0" spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <FlowPane alignment="CENTER" columnHalignment="CENTER" prefHeight="61.0" prefWidth="680.0">
               <children>
                  <Label text="Acompanhamento De Processos" textFill="WHITE">
                     <font>
                        <Font name="Verdana" size="14.0" />
                     </font>
                  </Label>
               </children>
            </FlowPane>
            <Label text="1ª Vara" textFill="WHITE">
               <font>
                  <Font name="Verdana" size="14.0" />
               </font>
            </Label>
            <TableView fx:id="tbl1Vara" prefHeight="200.0" prefWidth="665.0" tableMenuButtonVisible="true">
              <columns>
                <TableColumn fx:id="colProcesso1" prefWidth="75.0" text="Processo" />
                <TableColumn fx:id="colComarca1" prefWidth="75.0" text="Comarca" />
                  <TableColumn fx:id="colPartes1" prefWidth="75.0" text="Partes" />
                  <TableColumn fx:id="colAndamento1" prefWidth="75.0" text="Andamento" />
                  <TableColumn fx:id="colData1" prefWidth="75.0" text="Data" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <Label text="2ª Vara" textFill="WHITE">
               <font>
                  <Font name="Verdana" size="14.0" />
               </font>
            </Label>
            <TableView fx:id="tbl2Vara" prefHeight="200.0" prefWidth="665.0" tableMenuButtonVisible="true">
              <columns>
                <TableColumn fx:id="colProcesso2" prefWidth="75.0" text="Processo" />
                <TableColumn fx:id="colComarca2" prefWidth="75.0" text="Comarca" />
                  <TableColumn fx:id="colPartes2" prefWidth="75.0" text="Partes" />
                  <TableColumn fx:id="colAndamento2" prefWidth="75.0" text="Andamento" />
                  <TableColumn fx:id="colData2" prefWidth="75.0" text="Data" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <Label text="Vara Criminal" textFill="WHITE">
               <font>
                  <Font name="Verdana" size="14.0" />
               </font>
            </Label>
            <TableView fx:id="tbl3Vara" prefHeight="200.0" prefWidth="665.0" tableMenuButtonVisible="true">
              <columns>
                <TableColumn fx:id="colProcesso3" prefWidth="75.0" text="Processo" />
                <TableColumn fx:id="colComarca3" prefWidth="75.0" text="Comarca" />
                  <TableColumn fx:id="colPartes3" prefWidth="75.0" text="Partes" />
                  <TableColumn fx:id="colAndamento3" prefWidth="75.0" text="Andamento" />
                  <TableColumn fx:id="colData1" prefWidth="75.0" text="Data" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <ButtonBar prefHeight="40.0" prefWidth="200.0">
              <buttons>
                <Button fx:id="btnCadastrar" mnemonicParsing="false" onAction="#actionButton" text="Cadastrar Processo">
                     <font>
                        <Font name="Verdana" size="12.0" />
                     </font>
                  </Button>
              </buttons>
            </ButtonBar>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
