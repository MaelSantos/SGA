<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="700.0" style="-fx-background-color: #008B8B;" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.sga.controle.ControleCadastroAudiencia">
   <children>
      <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <FlowPane alignment="CENTER" columnHalignment="CENTER" hgap="10.0" orientation="VERTICAL" prefHeight="200.0" prefWidth="200.0" vgap="10.0" BorderPane.alignment="CENTER">
               <children>
                  <TextField fx:id="tfdProcesso" editable="false" promptText="Processo">
                     <font>
                        <Font name="Verdana" size="12.0" />
                     </font>
                  </TextField>
                  <ComboBox fx:id="cbxTipo" prefWidth="174.0" promptText="Tipo" />
                  <ComboBox fx:id="cbxStatus" prefWidth="174.0" promptText="Status" />
                  <TextField fx:id="tfdVara" promptText="Vara">
                     <font>
                        <Font name="Verdana" size="12.0" />
                     </font>
                  </TextField>
                  <TextField fx:id="tfdOrgao" promptText="Orgão">
                     <font>
                        <Font name="Verdana" size="12.0" />
                     </font>
                  </TextField>
                  <DatePicker fx:id="tfdData" promptText="Data Da Audiência" />
               </children>
            </FlowPane>
         </center>
         <bottom>
            <ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
              <buttons>
                  <Button fx:id="btnVoltar" mnemonicParsing="false" onAction="#actionButton" text="Voltar">
                     <font>
                        <Font name="Verdana" size="12.0" />
                     </font>
                  </Button>
                <Button fx:id="btnCadastrar" mnemonicParsing="false" onAction="#actionButton" text="Cadastrar Audiência">
                     <font>
                        <Font name="Verdana" size="12.0" />
                     </font>
                  </Button>
              </buttons>
            </ButtonBar>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
